{
  "manifest_version": 2,
  "name": "Spotify-NowPlaying",
  "description": "NowPlaying",
  "version": "0.0.1",
  "browser_action": {
  },
  "options_ui": {
    "page": "options/index.html",
    "open_in_tab": false
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "identity",
    "storage",
    "https://accounts.spotify.com/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
